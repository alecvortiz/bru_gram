<h1> Your Photos </h1>

<%= link_to 'Add Photo', new_user_photo_path(current_user) %>

<table>
	<% @photos.each do |photo| %>
		<tr>
			<td><%= photo.caption %> | </td>
			<td><%= link_to 'Show Photo', user_photo_path(current_user, photo) %> | </td>	
			<td><%= link_to 'Edit Photo', edit_user_photo_path(current_user, photo) %> | </td>
			<td><%= link_to 'Destroy Photo', [current_user, photo],
							method: :delete,
							data: { confirm: 'Are you sure?' } %></td>
		</tr>
	<% end %>
</table>



<%= link_to 'Back', '/' %>

